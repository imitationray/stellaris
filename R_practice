library(ggplot2)
library(dplyr)
library(MASS)

data(iris)
head(iris)

summary(iris)

mean(iris$Sepal.Length)
median(iris$Sepal.Length)
sd(iris$Sepal.Length)
var(iris$Sepal.Length)
quantile(iris$Sepal.Length)
quantile(iris$Sepal.Length, 1/4)
max(iris$Sepal.Length)
min(iris$Sepal.Length)

