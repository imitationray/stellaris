head(Animals)

mean(Animals$body)
mean(Animals$brain)
median(Animals$body)
median(Animals$brain)
sd(Animals$body)
sd(Animals$brain)
var(Animals$body)
var(Animals$brain)
quantile(Animals$body)
quantile(Animals$brain)

